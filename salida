# tests/unit/domain/use_cases/test_create_route_use_case.py
from unittest.mock import MagicMock

import pytest

from domain.models.route import Route
from domain.use_cases.create_route_use_case import CreateRouteUseCase

@pytest.fixture
def mock_repository():
    """Crea un mock del repositorio de rutas."""
    return MagicMock()

@pytest.fixture
def create_route_use_case(mock_repository):
    """Crea la instancia del UseCase con el mock."""
    return CreateRouteUseCase(route_repository=mock_repository)

def test_create_route_success(create_route_use_case, mock_repository):
    # Arrange: datos de una ruta válida
    route_data = {
        "id": None,
        "flightId": "AA001",
        "sourceAirportCode": "JFK",
        "sourceCountry": "USA",
        "destinyAirportCode": "LHR",
        "destinyCountry": "UK",
        "bagCost": 50,
        "plannedStartDate": "2025-08-22T10:00:00",
        "plannedEndDate": "2025-08-22T17:00:00",
    }
    route = Route(**route_data)
    mock_repository.create.return_value = route

    # Act
    result = create_route_use_case.execute(route)

    # Assert
    assert isinstance(result, Route)
    assert result.flightId == "AA001"
    mock_repository.create.assert_called_once_with(route)

def test_create_route_failure_raises(create_route_use_case, mock_repository):
    # Arrange: mock lanza un error
    route_data = {
        "id": None,
        "flightId": "AA002",
        "sourceAirportCode": "ORD",
        "sourceCountry": "USA",
        "destinyAirportCode": "CDG",
        "destinyCountry": "FR",
        "bagCost": 60,
        "plannedStartDate": "2025-08-23T08:00:00",
        "plannedEndDate": "2025-08-23T16:00:00",
    }
    route = Route(**route_data)
    mock_repository.create.side_effect = Exception("Error creando la ruta")

    # Act & Assert
    with pytest.raises(Exception, match="Error creando la ruta"):
        create_route_use_case.execute(route)
