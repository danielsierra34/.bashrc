# src/adapters/db/route_repository_adapter.py
from typing import List, Optional

from sqlalchemy.orm import Session

from domain.models.route import Route
from domain.ports.route_repository_port import RouteRepositoryPort
from errors import RouteNotFoundError

from .models import RouteORM

class PostgresRouteRepositoryAdapter(RouteRepositoryPort):
    """PostgreSQL implementation of RouteRepository."""

    def __init__(self, db: Session):
        self.db = db

    def create(self, route: Route) -> Route:
        orm_obj = RouteORM(**route.dict(exclude={"id"}))
        self.db.add(orm_obj)
        self.db.commit()
        self.db.refresh(orm_obj)
        return Route(**orm_obj.__dict__)

    def get_by_id(self, route_id: int) -> Optional[Route]:
        orm_obj = self.db.query(RouteORM).filter(RouteORM.id == route_id).first()
        return Route(**orm_obj.__dict__) if orm_obj else None

    def get_all(self) -> List[Route]:
        orm_objs = self.db.query(RouteORM).all()
        return [Route(**o.__dict__) for o in orm_objs]

    def update(self, route: Route) -> Route:
        orm_obj = self.db.query(RouteORM).filter(RouteORM.id == route.id).first()
        if not orm_obj:
            raise RouteNotFoundError(f"Route with id {route.id} not found")

        for field, value in route.dict().items():
            setattr(orm_obj, field, value)

        self.db.commit()
        self.db.refresh(orm_obj)
        return Route(**orm_obj.__dict__)

    def delete(self, route_id: int) -> Route:
        orm_obj = self.db.query(RouteORM).filter(RouteORM.id == route_id).first()
        if not orm_obj:
            raise RouteNotFoundError(f"Route with id {route_id} not found")

        self.db.delete(orm_obj)
        self.db.commit()
        return Route(**orm_obj.__dict__)

    def reset(self):
        """Elimina todas las rutas y reinicia la secuencia del ID."""
        self.db.execute("TRUNCATE TABLE routes RESTART IDENTITY CASCADE;")
        self.db.commit()
========== FIN DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/routes_app/src/adapters/db/route_repository_adapter.py ==========
