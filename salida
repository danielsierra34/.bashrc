from datetime import datetime, timedelta
from unittest.mock import MagicMock

import pytest

from domain.models.route import Route
from domain.use_cases.get_route_use_case import GetRouteUseCase

@pytest.fixture
def mock_repository():
    return MagicMock()

@pytest.fixture
def sample_route():
    now = datetime.utcnow()
    return Route(
        id=1,
        flightId="AA001",
        sourceAirportCode="JFK",
        sourceCountry="USA",
        destinyAirportCode="LHR",
        destinyCountry="UK",
        bagCost=50,
        plannedStartDate=now,
        plannedEndDate=now + timedelta(hours=7),
    )

@pytest.fixture
def use_case(mock_repository):
    return GetRouteUseCase(route_repository=mock_repository)

def test_execute_returns_route(use_case, mock_repository, sample_route):
    # Arrange
    mock_repository.get_by_id.return_value = sample_route

    # Act
    result = use_case.execute(1)

    # Assert
    assert result == sample_route
    mock_repository.get_by_id.assert_called_once_with(1)

def test_execute_returns_none_when_not_found(use_case, mock_repository):
    # Arrange
    mock_repository.get_by_id.return_value = None

    # Act
    result = use_case.execute(999)

    # Assert
    assert result is None
    mock_repository.get_by_id.assert_called_once_with(999)

def test_execute_raises_exception(use_case, mock_repository):
    # Arrange
    mock_repository.get_by_id.side_effect = Exception("DB error")

    # Act / Assert
    with pytest.raises(Exception) as exc_info:
        use_case.execute(1)

    assert str(exc_info.value) == "DB error"
    mock_repository.get_by_id.assert_called_once_with(1)
