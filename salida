# tests/unit/domain/use_cases/test_update_route_use_case.py
from datetime import datetime, timedelta
from unittest.mock import Mock

import pytest

from domain.models.route import Route
from domain.use_cases.update_route_use_case import UpdateRouteUseCase

@pytest.fixture
def mock_repository():
    return Mock()

@pytest.fixture
def use_case(mock_repository):
    return UpdateRouteUseCase(route_repository=mock_repository)

@pytest.fixture
def valid_route():
    now = datetime.utcnow()
    return Route(
        id=1,
        flightId="AA001",
        sourceAirportCode="JFK",
        sourceCountry="USA",
        destinyAirportCode="LHR",
        destinyCountry="UK",
        bagCost=100,
        plannedStartDate=now,
        plannedEndDate=now + timedelta(hours=6),
    )

def test_execute_calls_repository_update(use_case, mock_repository, valid_route):
    # Arrange
    mock_repository.update.return_value = valid_route

    # Act
    result = use_case.execute(valid_route)

    # Assert
    mock_repository.update.assert_called_once_with(valid_route)
    assert result == valid_route

def test_execute_returns_updated_route(use_case, mock_repository, valid_route):
    # Arrange
    updated_route = valid_route.copy(update={"bagCost": 200})
    mock_repository.update.return_value = updated_route

    # Act
    result = use_case.execute(valid_route)

    # Assert
    assert result.bagCost == 200
    assert result.destinyCountry == "UK"
    assert isinstance(result, Route)
