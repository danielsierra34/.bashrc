# tests/unit/adapters/memory/test_route_repository_adapter.py
import pytest

from adapters.memory.route_repository_adapter import InMemoryRouteRepositoryAdapter
from domain.models.route import Route
from errors import RouteNotFoundError

@pytest.fixture(autouse=True)
def clean_memory_store():
    """Limpia el memory_store antes de cada test para evitar contaminación entre pruebas."""
    InMemoryRouteRepositoryAdapter.memory_store.clear()
    yield
    InMemoryRouteRepositoryAdapter.memory_store.clear()

def make_route(**kwargs) -> Route:
    """Crea un objeto Route válido con valores por defecto."""
    defaults = dict(
        flightId="FL123",
        sourceAirportCode="BOG",
        sourceCountry="CO",
        destinyAirportCode="MIA",
        destinyCountry="US",
        bagCost=100,
        plannedStartDate="2025-01-01",
        plannedEndDate="2025-01-02",
    )
    defaults.update(kwargs)
    return Route(**defaults)

def test_sequence_increments_correctly():
    repo = InMemoryRouteRepositoryAdapter()
    assert repo.sequence() == 1
    repo.create(make_route(flightId="FL001"))
    assert repo.sequence() == 2

def test_create_route_assigns_id_and_stores_it():
    repo = InMemoryRouteRepositoryAdapter()
    route = make_route(flightId="FL002")
    created = repo.create(route)

    assert created.id == 1
    assert repo.memory_store[1] == created

def test_get_by_id_returns_existing_route():
    repo = InMemoryRouteRepositoryAdapter()
    route = repo.create(make_route(flightId="FL003"))

    fetched = repo.get_by_id(route.id)
    assert fetched == route

def test_get_by_id_returns_none_if_not_found():
    repo = InMemoryRouteRepositoryAdapter()
    assert repo.get_by_id(999) is None

def test_get_all_returns_all_routes():
    repo = InMemoryRouteRepositoryAdapter()
    r1 = repo.create(make_route(flightId="FL004"))
    r2 = repo.create(make_route(flightId="FL005"))

    all_routes = repo.get_all()
    assert len(all_routes) == 2
    assert r1 in all_routes and r2 in all_routes

def test_update_existing_route():
    repo = InMemoryRouteRepositoryAdapter()
    route = repo.create(make_route(flightId="FL006"))

    route.destinyCountry = "MX"
    updated = repo.update(route)

    assert updated.destinyCountry == "MX"
    assert repo.memory_store[route.id].destinyCountry == "MX"

def test_update_nonexistent_route_raises():
    repo = InMemoryRouteRepositoryAdapter()
    fake_route = make_route(id=999, flightId="FL999")

    with pytest.raises(RouteNotFoundError):
        repo.update(fake_route)

def test_delete_existing_route():
    repo = InMemoryRouteRepositoryAdapter()
    route = repo.create(make_route(flightId="FL007"))

    deleted = repo.delete(route.id)

    assert deleted == route
    assert repo.get_by_id(route.id) is None

def test_delete_nonexistent_route_raises():
    repo = InMemoryRouteRepositoryAdapter()
    with pytest.raises(RouteNotFoundError):
        repo.delete(1234)
