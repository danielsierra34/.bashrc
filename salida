import os

from sqlalchemy import create_engine
from sqlalchemy.orm import declarative_base, sessionmaker

# Lee la variable DATABASE_URL de entorno (usada en GitHub Actions)
DATABASE_URL = os.getenv(
    "DATABASE_URL",  # Ej: "postgresql+psycopg2://postgres:postgres@localhost:5432/routes_db"
    "postgresql+psycopg2://postgres:postgres@routes-db-service:5432/routes_db",
)

# Crear engine de SQLAlchemy
engine = create_engine(
    DATABASE_URL,
    echo=True,  # muestra SQL en consola (útil para debug, puedes poner False en prod)
    future=True,
)

# Configuración de sesión
SessionLocal = sessionmaker(autocommit=False, autoflush=False, bind=engine)

# Declarative Base para los modelos
Base = declarative_base()
