from datetime import datetime
from uuid import UUID

from pydantic import BaseModel

class Post(BaseModel):
    id: UUID
    routeId: UUID
    userId: UUID
    expireAt: datetime
    createdAt: datetime

    class Config:
        from_attributes = True

class PostCreate(BaseModel):
    routeId: UUID
    userId: UUID
    expireAt: datetime

class PostResponse(BaseModel):
    id: UUID
    userId: UUID
    createdAt: datetime
