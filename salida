from datetime import datetime

from pydantic import BaseModel, Field

class Route(BaseModel):
    """Route domain model."""

    id: int | None = None
    flightId: str = Field(
        min_length=1, description="Identificador único del vuelo, p. ej. AA001"
    )
    sourceAirportCode: str = Field(
        min_length=3,
        max_length=5,
        description="Código del aeropuerto de origen (IATA/ICAO)",
    )
    sourceCountry: str = Field(min_length=1, description="País de origen")
    destinyAirportCode: str = Field(
        min_length=3,
        max_length=5,
        description="Código del aeropuerto de destino (IATA/ICAO)",
    )
    destinyCountry: str = Field(min_length=1, description="País de destino")
    bagCost: int = Field(ge=0, description="Costo del envío de una maleta (entero ≥ 0)")
    plannedStartDate: datetime = Field(
        description="Fecha/hora planeada de inicio (ISO, UTC)"
    )
    plannedEndDate: datetime = Field(
        description="Fecha/hora planeada de fin (ISO, UTC)"
    )
