# tests/unit/domain/use_cases/test_delete_route_use_case.py
from unittest.mock import MagicMock

import pytest

from domain.models.route import Route
from domain.use_cases.delete_route_use_case import DeleteRouteUseCase

@pytest.fixture
def mock_repository():
    """Crea un mock del repositorio de rutas."""
    return MagicMock()

@pytest.fixture
def delete_route_use_case(mock_repository):
    """Crea la instancia del UseCase con el mock."""
    return DeleteRouteUseCase(route_repository=mock_repository)

def test_delete_route_success(delete_route_use_case, mock_repository):
    # Arrange: ruta existente a eliminar
    route = Route(
        id=1,
        flightId="AA001",
        sourceAirportCode="JFK",
        sourceCountry="USA",
        destinyAirportCode="LHR",
        destinyCountry="UK",
        bagCost=50,
        plannedStartDate="2025-08-22T10:00:00",
        plannedEndDate="2025-08-22T17:00:00",
    )
    mock_repository.delete.return_value = route

    # Act
    result = delete_route_use_case.execute(route.id)

    # Assert
    assert result.id == 1
    assert result.flightId == "AA001"
    mock_repository.delete.assert_called_once_with(route.id)

def test_delete_route_not_found(delete_route_use_case, mock_repository):
    # Arrange: ruta no existe
    mock_repository.delete.side_effect = ValueError("Route not found")

    # Act & Assert
    with pytest.raises(ValueError, match="Route not found"):
        delete_route_use_case.execute(999)
