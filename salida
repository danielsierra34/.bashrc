        env:
          - name: POSTGRES_USER
            valueFrom:
              secretKeyRef:
                name: users-app-secret
                key: POSTGRES_USER
          - name: POSTGRES_HOST
            valueFrom:
              secretKeyRef:
                name: users-app-secret
                key: POSTGRES_HOST
