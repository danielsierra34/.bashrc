========== INICIO DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/routes_app/tests/unit/entrypoints/api/routers/test_route_router.py ==========
import pytest
from fastapi import FastAPI
from fastapi.testclient import TestClient

# Importamos directamente el router
from entrypoints.api.routers import route_router

@pytest.fixture
def app():
    """Crea una aplicación FastAPI con el router incluido para pruebas."""
    app = FastAPI()
    app.include_router(route_router.router)
    return app

@pytest.fixture
def client(app):
    """Crea un cliente de prueba para la app."""
    return TestClient(app)

def test_router_inclusion(app):
    """Verifica que el router se haya incluido en la aplicación."""
    routes = [route.path for route in app.routes]
    # Aseguramos que existe al menos una ruta del router
    assert any("/" in path for path in routes)

def test_openapi_schema_contains_router(app):
    """Verifica que la ruta del router aparezca en el esquema OpenAPI."""
    schema = app.openapi()
    paths = schema.get("paths", {})
    assert isinstance(paths, dict)
    assert any(isinstance(key, str) for key in paths.keys())

def test_health_endpoint_if_exists(client):
    """
    Intenta acceder a un endpoint de salud típico.
    Si no existe, se maneja correctamente.
    """
    response = client.get("/health")
    if response.status_code == 200:
        body = response.json()
        assert "status" in body or isinstance(body, dict)
    else:
        # El endpoint no existe, probamos que al menos es 404
        assert response.status_code in (404, 405)

def test_root_endpoint_if_exists(client):
    """
    Intenta acceder al endpoint raíz.
    Si existe, valida su contenido.
    """
    response = client.get("/")
    if response.status_code == 200:
        assert response.json() is not None
    else:
        assert response.status_code in (404, 405)

def test_invalid_method_on_known_path(client):
    """
    Verifica que un método no permitido en un path existente devuelva 405.
    """
    # Buscamos alguna ruta válida registrada
    schema = client.get("/openapi.json").json()
    paths = list(schema.get("paths", {}).keys())
    if paths:
        path = paths[0]
        response = client.post(path)  # Forzamos POST en un endpoint posiblemente GET
        assert response.status_code in (405, 422, 200)
    else:
        pytest.skip("No hay rutas registradas en el router.")
