  name: users-network
spec:
  podSelector:
    matchLabels:
      app: users-db
  policyTypes:
  - Ingress
  - Egress
  ingress:
  - from:
    - podSelector:
        matchLabels:
          app: users-app
    ports:
    - protocol: TCP
      port: 5432
