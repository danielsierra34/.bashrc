class PostNotFoundError(Exception):
    """Exception raised when a pet is not found."""

    pass
========== FIN DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/posts_app/src/errors.py ==========
