# src/entrypoints/api/main.py
from fastapi import FastAPI

from adapters.db.database import Base, engine
from config import Settings  # <-- importa la configuración
from entrypoints.api.routers import route_router

settings = Settings()

app = FastAPI(title=settings.app_name)  # <-- usa el nombre desde config

# Crear tablas si no existen
Base.metadata.create_all(bind=engine)

app.include_router(route_router.router)
========== FIN DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/routes_app/src/entrypoints/api/main.py ==========
