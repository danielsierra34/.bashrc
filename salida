# tests/unit/test_assembly.py

import assembly
from adapters.memory.route_repository_adapter import InMemoryRouteRepositoryAdapter
from domain.use_cases.create_route_use_case import CreateRouteUseCase
from domain.use_cases.delete_route_use_case import DeleteRouteUseCase
from domain.use_cases.get_route_use_case import GetRouteUseCase
from domain.use_cases.get_routes_use_case import GetAllRoutesUseCase
from domain.use_cases.update_route_use_case import UpdateRouteUseCase

def test_repository_is_singleton_instance():
    """El repositorio en assembly debe ser una instancia única de InMemoryRouteRepositoryAdapter."""
    repo1 = assembly.repository
    repo2 = assembly.repository
    assert isinstance(repo1, InMemoryRouteRepositoryAdapter)
    assert repo1 is repo2  # Singleton: debe ser el mismo objeto en memoria

def test_build_create_route_use_case_returns_correct_type():
    uc = assembly.build_create_route_use_case()
    assert isinstance(uc, CreateRouteUseCase)
    assert uc.route_repository is assembly.repository

def test_build_get_route_use_case_returns_correct_type():
    uc = assembly.build_get_route_use_case()
    assert isinstance(uc, GetRouteUseCase)
    assert uc.route_repository is assembly.repository

def test_build_get_routes_use_case_returns_correct_type():
    uc = assembly.build_get_routes_use_case()
    assert isinstance(uc, GetAllRoutesUseCase)
    assert uc.route_repository is assembly.repository

def test_build_update_route_use_case_returns_correct_type():
    uc = assembly.build_update_route_use_case()
    assert isinstance(uc, UpdateRouteUseCase)
    assert uc.route_repository is assembly.repository

def test_build_delete_route_use_case_returns_correct_type():
    uc = assembly.build_delete_route_use_case()
    assert isinstance(uc, DeleteRouteUseCase)
    assert uc.route_repository is assembly.repository
