from datetime import datetime
from unittest.mock import MagicMock

import pytest

from domain.models.route import Route
from domain.use_cases.get_routes_use_case import GetAllRoutesUseCase

@pytest.fixture
def mock_route_repository():
    """Crea un mock del repositorio de rutas."""
    return MagicMock()

@pytest.fixture
def get_all_routes_use_case(mock_route_repository):
    """Instancia el caso de uso con el repositorio mockeado."""
    return GetAllRoutesUseCase(route_repository=mock_route_repository)

def make_route(id: int, name: str) -> Route:
    """Helper para crear un objeto Route válido."""
    return Route(
        id=id,
        flightId=f"FL{id}",
        name=name,
        sourceAirportCode="BOG",
        sourceCountry="Colombia",
        destinyAirportCode="MIA",
        destinyCountry="USA",
        bagCost=50.0,
        plannedStartDate=datetime(2025, 1, 1, 10, 0, 0),
        plannedEndDate=datetime(2025, 1, 1, 14, 0, 0),
    )

def test_execute_returns_all_routes(get_all_routes_use_case, mock_route_repository):
    # Arrange
    routes = [
        make_route(1, "Route A"),
        make_route(2, "Route B"),
    ]
    mock_route_repository.get_all.return_value = routes

    # Act
    result = get_all_routes_use_case.execute()

    # Assert
    assert result == routes
    mock_route_repository.get_all.assert_called_once()

def test_execute_returns_empty_list(get_all_routes_use_case, mock_route_repository):
    # Arrange
    mock_route_repository.get_all.return_value = []

    # Act
    result = get_all_routes_use_case.execute()

    # Assert
    assert result == []
    mock_route_repository.get_all.assert_called_once()

def test_execute_raises_exception(get_all_routes_use_case, mock_route_repository):
    # Arrange
    mock_route_repository.get_all.side_effect = Exception("DB Error")

    # Act / Assert
    with pytest.raises(Exception, match="DB Error"):
        get_all_routes_use_case.execute()
