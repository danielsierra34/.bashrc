# src/adapters/db/models.py
from sqlalchemy import Column, DateTime, Integer, String

from .database import Base

class RouteORM(Base):
    __tablename__ = "routes"

    id = Column(Integer, primary_key=True, index=True, autoincrement=True)
    flightId = Column(String, nullable=False, unique=True, index=True)
    sourceAirportCode = Column(String(5), nullable=False)
    sourceCountry = Column(String, nullable=False)
    destinyAirportCode = Column(String(5), nullable=False)
    destinyCountry = Column(String, nullable=False)
    bagCost = Column(Integer, nullable=False, default=0)
    plannedStartDate = Column(DateTime(timezone=True), nullable=False)
    plannedEndDate = Column(DateTime(timezone=True), nullable=False)
