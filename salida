========== FIN DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/routes_app/src/adapters/memory/__init__.py ==========
========== INICIO DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/routes_app/src/adapters/memory/route_repository_adapter.py ==========
from typing import Dict, List, Optional

from domain.models.route import Route
from domain.ports.route_repository_port import RouteRepositoryPort
from errors import RouteNotFoundError

class InMemoryRouteRepositoryAdapter(RouteRepositoryPort):
    """In memory implementation of RouteRepository."""

    memory_store: Dict[int, Route] = {}

    def sequence(self) -> int:
        """Generate a new sequence number."""
        return len(self.memory_store) + 1

    def create(self, route: Route) -> Route:
        """Create a new route."""
        route.id = self.sequence()
        self.memory_store[route.id] = route
        return route

    def get_by_id(self, route_id: int) -> Optional[Route]:
        """Get route by ID."""
        return self.memory_store.get(route_id)

    def get_all(self) -> List[Route]:
        """Get all routes."""
        return list(self.memory_store.values())

    def update(self, route: Route) -> Route:
        """Update an existing route."""
        if route.id not in self.memory_store:
            raise RouteNotFoundError(f"Route with id {route.id} not found")
        self.memory_store[route.id] = route
        return route

    def delete(self, route_id: int) -> Route:
        """Delete a route."""
        if route_id not in self.memory_store:
            raise RouteNotFoundError(f"Route with id {route_id} not found")
        route = self.memory_store[route_id]
        del self.memory_store[route_id]
        return route
========== FIN DE: /home/ubuntu24/MAESTRIA/misw4301-aplicaciones_nativas_nube/X/entrega/routes_app/src/adapters/memory/route_repository_adapter.py ==========
