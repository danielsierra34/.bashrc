from datetime import datetime, timedelta

import pytest
from pydantic import ValidationError

from domain.models.route import Route

def test_create_route_with_valid_data(valid_route_data):
    """Test creating a route with valid data."""
    route = Route(**valid_route_data)
    for key, value in valid_route_data.items():
        assert getattr(route, key) == value

def test_create_route_with_id(route_with_id):
    """Test creating a route with an ID."""
    for key, value in route_with_id.model_dump().items():
        assert getattr(route_with_id, key) == value

def test_create_route_with_missing_required_field(valid_route_data):
    """Test creating a route with missing required fields."""
    required_fields = [
        "flightId",
        "sourceAirportCode",
        "sourceCountry",
        "destinyAirportCode",
        "destinyCountry",
        "bagCost",
        "plannedStartDate",
        "plannedEndDate",
    ]
    for field in required_fields:
        data = valid_route_data.copy()
        data[field] = None  # type: ignore
        with pytest.raises(ValidationError) as exc_info:
            Route(**data)
        assert field in str(exc_info.value)

def test_create_route_with_negative_bag_cost(valid_route_data):
    """Test creating a route with a negative bag cost."""
    data = valid_route_data.copy()
    data["bagCost"] = -10
    with pytest.raises(ValidationError) as exc_info:
        Route(**data)
    assert "bagCost" in str(exc_info.value)

def test_create_route_with_end_before_start(valid_route_data):
    """Test creating a route with plannedEndDate before plannedStartDate."""
    data = valid_route_data.copy()
    data["plannedStartDate"] = datetime.utcnow()
    data["plannedEndDate"] = datetime.utcnow() - timedelta(hours=1)
    route = Route(**data)
    # Optional: add model validator to enforce this rule
    assert route.plannedEndDate < datetime.utcnow()

def test_route_model_equality(route_with_id):
    """Test that two routes with the same data are equal."""
    route2 = Route(**route_with_id.model_dump())
    assert route_with_id == route2

def test_route_model_inequality(route_with_id, valid_route_data):
    """Test that two routes with different IDs are not equal."""
    route2 = Route(**{**valid_route_data, "id": "2"})
    assert route_with_id != route2

def test_route_model_dict_conversion(route_with_id):
    """Test conversion of route model to dictionary."""
    route_dict = route_with_id.model_dump()
    for key, value in route_with_id.model_dump().items():
        assert route_dict[key] == value

def test_route_model_json_conversion(route_with_id):
    """Test conversion of route model to JSON."""
    route_json = route_with_id.model_dump_json()
    for key in route_with_id.model_dump().keys():
        assert f'"{key}"' in route_json

def test_route_with_extra_fields(valid_route_data):
    """Test that extra fields are ignored by Pydantic."""
    route = Route(**valid_route_data, extra_field="extra")  # type: ignore
    assert not hasattr(route, "extra_field")
